        tools:layout="@layout/fragment_writing_task_1_explanation" >
        <argument
            android:name="question"
            app:argType="string" />
        <argument
            android:name="questionImageName"
            app:argType="string" />
        <argument
            android:name="answer"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/writingTask2ExplanationFragment"
        android:name="com.ielts.preparation.ui.writing.writingTasks.fragment.tasks.WritingTask2ExplanationFragment"
