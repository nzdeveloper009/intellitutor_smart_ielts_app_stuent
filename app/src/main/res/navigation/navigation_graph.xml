        android:id="@+id/speakingLessonMainTopicsFragment"
        android:name="com.ielts.preparation.ui.speaking.speakingLessons.fragment.speakingLessonMainTopics.SpeakingLessonMainTopicsFragment"
        android:label="fragment_speaking_lesson_main_topics"
        tools:layout="@layout/fragment_speaking_lesson_main_topics" >
        <argument
            android:name="speakingMainTopics"
            app:argType="com.ielts.preparation.data.LessonItems[]" />
        <action
