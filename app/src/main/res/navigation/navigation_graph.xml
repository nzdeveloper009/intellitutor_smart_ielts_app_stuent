        android:id="@+id/speakingLessonMainTopicExplanationFragment"
        android:name="com.ielts.preparation.ui.speaking.speakingLessons.fragment.speakingLessonMainTopicExplanations.SpeakingLessonMainTopicExplanationFragment"
